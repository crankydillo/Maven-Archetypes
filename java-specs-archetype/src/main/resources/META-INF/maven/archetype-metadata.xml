<archetype-descriptor name="basic">
  <fileSets>
    <fileSet filtered="true" packaged="true">
      <directory>src/main/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" packaged="true">
      <directory>src/test/scala</directory>
      <includes>
        <include>**/*.scala</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>
